{
  "name": "gbiagomba.github.io",
  "version": "0.2.0",
  "description": "Neo-futurist security portfolio for Gilles S. Biagomba",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "format": "prettier --write .",
    "clean": "rm -rf dist .vite",
    "clean:all": "rm -rf dist node_modules .vite",
    "verify": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbiagomba/gbiagomba.github.io.git"
  },
  "keywords": [
    "security",
    "portfolio",
    "vite",
    "react",
    "tailwind"
  ],
  "author": "Gilles S. Biagomba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbiagomba/gbiagomba.github.io/issues"
  },
  "homepage": "https://gbiagomba.github.io",
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.76",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
